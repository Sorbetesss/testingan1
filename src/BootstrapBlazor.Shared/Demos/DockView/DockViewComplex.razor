@page "/dockview-complex"

<DockView Name="DockViewComplex" EnableLocalStorage="false">
    <DockContent Type="DockContentType.Row">
        <DockComponent Title="标签一">
            <BootstrapInput TValue="string"></BootstrapInput>
        </DockComponent>
        <DockComponent Title="标签二">
            <Button Text="Click"></Button>
        </DockComponent>
        <DockContent Type="DockContentType.Column">
            <DockComponent Title="标签三">
                <FetchData></FetchData>
            </DockComponent>
            <DockComponent Title="标签四">
                <Counter></Counter>
            </DockComponent>
        </DockContent>
    </DockContent>
</DockView>
