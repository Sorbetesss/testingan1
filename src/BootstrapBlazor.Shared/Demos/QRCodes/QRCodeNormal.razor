@inject IStringLocalizer<QRCodes> Localizer

<QRCode OnGenerated="@(()=>{Logger.Log(Localizer[nameof(SuccessText)]);return Task.CompletedTask;})" ShowButtons="true"></QRCode>
<ConsoleLogger @ref="Logger" class="mt-3" />

@code {
    [NotNull]
    private string? SuccessText { get; set; }

    [NotNull]
    private ConsoleLogger? Logger { get; set; }
}
