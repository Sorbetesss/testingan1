{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "LgbFile": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Error"
      },
      "FileName": "Error\\Log.log"
    }
  },
  "AllowedHosts": "*",
  "AllowOrigins": "http://localhost:50855,http://wasm.blazor.zone,https://wasm.blazor.zone",
  "ConnectionStrings": {
    "bb": "Data Source=bb.db;"
  },
  "BootstrapBlazorOptions": {
    "ToastDelay": 4000,
    "MessageDelay": 4000,
    "SwalDelay": 4000,
    "EnableErrorLogger": true,
    "FallbackCulture": "en",
    "SupportedCultures": [
      "zh-CN",
      "en-US"
    ],
    "TableSettings": {
      "CheckboxColumnWidth": 36
    },
    "IgnoreLocalizerMissing": true,
    "StepSettings": {
      "Short": 1,
      "Int": 1,
      "Long": 1,
      "Float": "0.1",
      "Double": "0.01",
      "Decimal": "0.01"
    }
  },
  "Cache-Control": {
    "Files": [ ".png", ".gif", ".jpg", ".jpeg", ".svg" ],
    "Max-Age": 600
  },
  "WebsiteOptions": {
    "ServerUrl": "https://www.blazor.zone",
    "AdminUrl": "https://admin.blazor.zone",
    "ImageLibUrl": "https://imgs.blazor.zone",
    "WasmUrl": "https://wasm.blazor.zone",
    "TotalCount": 160
  },
  "Themes": {
    "Ant Design (完善中)": "ant",
    "LayUI (完善中)": "layui",
    "DevUI (制作中)": "devui"
  },
  "WebHooks": {
    "Gitee": {
      "Id": "",
      "Token": ""
    }
  },
  "AzureSpeechOption": {
    "SubscriptionKey": "<SubscriptionKey>",
    "Region": "<Region>",
    "AuthorizationTokenUrl": "https://{0}.api.cognitive.microsoft.com/sts/v1.0/issueToken"
  },
  "BaiduSpeechOption": {
    "AppId": "<AppId>",
    "ApiKey": "<ApiKey>",
    "Secret": "<Secret>"
  },
  "BaiduOcrOption": {
    "AppId": "<AppId>",
    "ApiKey": "<ApiKey>",
    "Secret": "<Secret>"
  },
  "AzureOpenAIOption": {
    "Endpoint": "<Endpoint>",
    "Key": "<key>",
    "DeploymentName": "<DeploymentName>"
  },
  "GiteeOptions": {
    "ClientId": "<ClientId>",
    "ClientSecret": "<ClientSecret>",
    "CallbackPath": "/",
    "Scope": "<Scope>"
  },
  "GitHubOptions": {
    "ClientId": "<ClientId>",
    "ClientSecret": "<ClientSecret>",
    "CallbackPath": "/",
    "Scope": "<Scope>"
  }
}
